<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#111111">

    <View
        android:id="@+id/view8"
        android:layout_width="3dp"
        android:layout_height="0dp"
        android:layout_marginStart="248dp"
        android:background="#111112"
        android:backgroundTint="#111112"
        android:elevation="56dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view7"
        android:layout_width="3dp"
        android:layout_height="0dp"
        android:layout_marginStart="66dp"
        android:background="#111112"
        android:backgroundTint="#111112"
        android:elevation="56dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view6"
        android:layout_width="3dp"
        android:layout_height="0dp"
        android:layout_marginEnd="82dp"
        android:background="#111112"
        android:backgroundTint="#111112"
        android:elevation="56dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView11"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="2dp"
        android:elevation="12dp"
        app:cardBackgroundColor="@color/coral"
        app:cardCornerRadius="4dp"
        app:cardElevation="12dp"
        app:cardMaxElevation="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/n"
                android:layout_width="163dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:layout_marginEnd="1dp"
                android:background="@android:color/transparent"
                android:gravity="center"
                android:padding="8dp"
                android:text="Mazza \n 1count=1.2L"
                android:textColor="#D9FFFFFF"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/r"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/r"
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="1dp"
                android:background="@android:color/transparent"
                android:gravity="center|center_horizontal|center_vertical"
                android:padding="8dp"
                android:text="55 Rs"
                android:textColor="#D9FFFFFF"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/q"
                app:layout_constraintStart_toEndOf="@+id/n"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/q"
                android:layout_width="90.5dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="1dp"
                android:background="@android:color/transparent"
                android:gravity="center|center_horizontal"
                android:padding="8dp"
                android:text="3 Count \n(3.6L)"
                android:textColor="#D9FFFFFF"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/p"
                app:layout_constraintStart_toEndOf="@+id/r"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/p"
                android:layout_width="78dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="2dp"
                android:background="@android:color/transparent"
                android:gravity="center|center_horizontal|center_vertical"
                android:paddingStart="6dp"
                android:paddingTop="8dp"
                android:paddingEnd="10dp"
                android:paddingBottom="8dp"
                android:text="165 Rs"
                android:textColor="#F0A732"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/q"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>